@model SourceCode.Models.AdminTbl

@{
    ViewData["Title"] = "Sign Up for Admins";
}
<div class="form-section-SEES">
    <div>
        <form asp-action="SignUp" method="post">
            <div class="form-group">
                <label asp-for="Id">Admin ID</label>
                <input asp-for="Id" class="form-control" placeholder="Please Enter Your ID" required/>
            </div>
            <div class="form-group">
                <label asp-for="Email">Email</label>
                <input asp-for="Email" type="email" class="form-control" placeholder="Please Enter Your Email" required/>
            </div>
            <div class="form-group">
                <label asp-for="Password">Password</label>
                <input asp-for="Password" type="password" class="form-control"
                    placeholder="Please Enter the Password" required/>
            </div>
            <br />
            <div>
                <input type="submit" class="btn btn-primary" value="Submit" />
            </div>
        </form>
    </div>
    <div>
        <img class="form-image-SEES" src="~/img/SeeS_logo.png" alt="SEES_Logo" />
    </div>
</div>
